{
  "name": "cloudmusic",
  "displayName": "Cloudmusic",
  "description": "Netease Music for VS Code",
  "version": "9.17.1",
  "publisher": "YXL",
  "private": true,
  "preview": false,
  "workspaces": [
    "packages/*",
    "packages/@types/*"
  ],
  "engines": {
    "vscode": "^1.67.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "categories": [
    "Other"
  ],
  "capabilities": {
    "virtualWorkspaces": false,
    "untrustedWorkspaces": {
      "supported": true
    }
