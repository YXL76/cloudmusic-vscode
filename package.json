      "view/item/context": [
        {
          "command": "cloudmusic.refreshLocalFile",
          "group": "inline",
          "when": "view == local && viewItem == LocalLibraryTreeItem"
        },
        {
          "command": "cloudmusic.deleteLocalLibrary",
