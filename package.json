{
  "name": "cloudmusic",
  "displayName": "cloudmusic",
  "description": "Netease Music for VS Code",
  "version": "5.2.3",
  "publisher": "YXL",
  "engines": {
    "vscode": "^1.49.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "categories": [
    "Other"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YXL76/cloudmusic-vscode"
  },
  "keywords": [
    "网易云",
    "网易云音乐",
    "music",
    "cloudmusic",
    "Netease"
  ],
  "preview": false,
  "activationEvents": [
    "onView:userPlaylist",
    "onView:favoritePlaylist",
    "onView:queue"
  ],
  "icon": "media/icon-300.png",
  "main": "./dist/extension.js",
  "targets": {
    "main": false
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cloudmusic",
          "title": "%cloudmusic.viewsContainers.activitybar.cloudmusic.title%",
          "icon": "media/disc.svg"
        }
      ]
    },
    "views": {
      "cloudmusic": [
        {
          "id": "userPlaylist",
          "name": "%cloudmusic.views.cloudmusic.userPlaylist.name%"
        },
        {
          "id": "favoritePlaylist",
          "name": "%cloudmusic.views.cloudmusic.favoritePlaylist.name%"
        },
        {
          "id": "queue",
          "name": "%cloudmusic.views.cloudmusic.queue.name%"
        }
      ]
    },
    "commands": [
      {
        "command": "cloudmusic.signin",
        "title": "%cloudmusic.commands.cloudmusic.signin.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.signout",
        "title": "%cloudmusic.commands.cloudmusic.signout.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.dailyCheck",
        "title": "%cloudmusic.commands.cloudmusic.dailyCheck.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.account",
        "title": "%cloudmusic.commands.cloudmusic.account.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.previous",
        "title": "%cloudmusic.commands.cloudmusic.previous.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.next",
        "title": "%cloudmusic.commands.cloudmusic.next.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.play",
        "title": "%cloudmusic.commands.cloudmusic.play.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.like",
        "title": "%cloudmusic.commands.cloudmusic.like.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.volume",
        "title": "%cloudmusic.commands.cloudmusic.volume.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.toggleButton",
        "title": "%cloudmusic.commands.cloudmusic.toggleButton.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.personalFM",
        "title": "%cloudmusic.commands.cloudmusic.personalFM.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.search",
        "title": "%cloudmusic.commands.cloudmusic.search.title%",
        "category": "Cloudmusic"
      },
      {
        "command": "cloudmusic.refreshPlaylist",
        "title": "%cloudmusic.commands.cloudmusic.refreshPlaylist.title%",
        "icon": "$(refresh)"
      },
      {
        "command": "cloudmusic.createPlaylist",
        "title": "%cloudmusic.commands.cloudmusic.createPlaylist.title%",
        "icon": "$(add)"
      },
      {
        "command": "cloudmusic.refreshPlaylistContent",
        "title": "%cloudmusic.commands.cloudmusic.refreshPlaylistContent.title%",
        "icon": "$(refresh)"
      },
      {
        "command": "cloudmusic.playPlaylist",
        "title": "%cloudmusic.commands.cloudmusic.playPlaylist.title%",
        "icon": "$(play)"
      },
      {
        "command": "cloudmusic.addPlaylist",
        "title": "%cloudmusic.commands.cloudmusic.addPlaylist.title%",
        "icon": "$(plus)"
      },
      {
        "command": "cloudmusic.deletePlaylist",
        "title": "%cloudmusic.commands.cloudmusic.deletePlaylist.title%"
      },
      {
        "command": "cloudmusic.editPlaylist",
        "title": "%cloudmusic.commands.cloudmusic.editPlaylist.title%"
      },
      {
        "command": "cloudmusic.unsavePlaylist",
        "title": "%cloudmusic.commands.cloudmusic.unsavePlaylist.title%"
      },
      {
        "command": "cloudmusic.sortQueue",
        "title": "%cloudmusic.commands.cloudmusic.sortQueue.title%",
        "icon": "$(ungroup-by-ref-type)"
      },
      {
        "command": "cloudmusic.clearQueue",
        "title": "%cloudmusic.commands.cloudmusic.clearQueue.title%",
        "icon": "$(clear-all)"
      },
      {
        "command": "cloudmusic.randomQueue",
        "title": "%cloudmusic.commands.cloudmusic.randomQueue.title%",
        "icon": "$(sort-precedence)"
      },
      {
        "command": "cloudmusic.intelligence",
        "title": "%cloudmusic.commands.cloudmusic.intelligence.title%",
        "icon": "$(pulse)"
      },
      {
        "command": "cloudmusic.addSong",
        "title": "%cloudmusic.commands.cloudmusic.addSong.title%",
        "icon": "$(add)"
      },
      {
        "command": "cloudmusic.playSongWithPlaylist",
        "title": "%cloudmusic.commands.cloudmusic.playSongWithPlaylist.title%",
        "icon": "$(play)"
      },
      {
        "command": "cloudmusic.playSong",
        "title": "%cloudmusic.commands.cloudmusic.playSong.title%",
        "icon": "$(play)"
      },
      {
        "command": "cloudmusic.deleteSong",
        "title": "%cloudmusic.commands.cloudmusic.deleteSong.title%",
        "icon": "$(remove)"
      },
      {
        "command": "cloudmusic.playNext",
        "title": "%cloudmusic.commands.cloudmusic.playNext.title%",
        "icon": "$(arrow-up)"
      },
      {
        "command": "cloudmusic.deleteFromPlaylist",
        "title": "%cloudmusic.commands.cloudmusic.deleteFromPlaylist.title%"
      },
      {
        "command": "cloudmusic.saveToPlaylist",
        "title": "%cloudmusic.commands.cloudmusic.saveToPlaylist.title%"
      },
      {
        "command": "cloudmusic.songDetail",
        "title": "%cloudmusic.commands.cloudmusic.songDetail.title%"
      },
      {
        "command": "cloudmusic.playlistDetail",
        "title": "%cloudmusic.commands.cloudmusic.playlistDetail.title%"
      },
      {
        "command": "cloudmusic.songComment",
        "title": "%cloudmusic.commands.cloudmusic.songComment.title%"
      },
      {
        "command": "cloudmusic.playlistComment",
        "title": "%cloudmusic.commands.cloudmusic.playlistComment.title%"
      },
      {
        "command": "cloudmusic.lyric",
        "title": "%cloudmusic.commands.cloudmusic.lyric.title%"
      },
      {
        "command": "cloudmusic.fmTrash",
        "title": "%cloudmusic.commands.cloudmusic.fmTrash.title%"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "cloudmusic.refreshPlaylist",
          "when": "view == userPlaylist || view == favoritePlaylist",
          "group": "navigation"
        },
        {
          "command": "cloudmusic.createPlaylist",
          "when": "view == userPlaylist",
          "group": "navigation"
        },
        {
          "command": "cloudmusic.sortQueue",
          "when": "view == queue",
          "group": "navigation"
        },
        {
          "command": "cloudmusic.clearQueue",
          "when": "view == queue",
          "group": "navigation"
        },
        {
          "command": "cloudmusic.randomQueue",
          "when": "view == queue",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "cloudmusic.refreshPlaylistContent",
          "when": "view == userPlaylist && viewItem == PlaylistItemTreeItem || view == favoritePlaylist && viewItem == PlaylistItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.refreshPlaylistContent",
          "when": "view == userPlaylist && viewItem == PlaylistItemTreeItem || view == favoritePlaylist && viewItem == PlaylistItemTreeItem"
        },
        {
          "command": "cloudmusic.playPlaylist",
          "when": "view == userPlaylist && viewItem == PlaylistItemTreeItem || view == favoritePlaylist && viewItem == PlaylistItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.playPlaylist",
          "when": "view == userPlaylist && viewItem == PlaylistItemTreeItem || view == favoritePlaylist && viewItem == PlaylistItemTreeItem"
        },
        {
          "command": "cloudmusic.addPlaylist",
          "when": "view == userPlaylist && viewItem == PlaylistItemTreeItem || view == favoritePlaylist && viewItem == PlaylistItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.addPlaylist",
          "when": "view == userPlaylist && viewItem == PlaylistItemTreeItem || view == favoritePlaylist && viewItem == PlaylistItemTreeItem"
        },
        {
          "command": "cloudmusic.deletePlaylist",
          "when": "view == userPlaylist && viewItem == PlaylistItemTreeItem"
        },
        {
          "command": "cloudmusic.editPlaylist",
          "when": "view == userPlaylist && viewItem == PlaylistItemTreeItem"
        },
        {
          "command": "cloudmusic.unsavePlaylist",
          "when": "view == favoritePlaylist && viewItem == PlaylistItemTreeItem"
        },
        {
          "command": "cloudmusic.addSong",
          "when": "view == userPlaylist && viewItem == QueueItemTreeItem || view == favoritePlaylist && viewItem == QueueItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.addSong",
          "when": "view == userPlaylist && viewItem == QueueItemTreeItem || view == favoritePlaylist && viewItem == QueueItemTreeItem"
        },
        {
          "command": "cloudmusic.intelligence",
          "when": "view == userPlaylist && viewItem == QueueItemTreeItem || view == favoritePlaylist && viewItem == QueueItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.intelligence",
          "when": "view == userPlaylist && viewItem == QueueItemTreeItem || view == favoritePlaylist && viewItem == QueueItemTreeItem"
        },
        {
          "command": "cloudmusic.playSongWithPlaylist",
          "when": "view == userPlaylist && viewItem == QueueItemTreeItem || view == favoritePlaylist && viewItem == QueueItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.playSongWithPlaylist",
          "when": "view == userPlaylist && viewItem == QueueItemTreeItem || view == favoritePlaylist && viewItem == QueueItemTreeItem"
        },
        {
          "command": "cloudmusic.playSong",
          "when": "view == queue && viewItem == QueueItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.playSong",
          "when": "view == queue && viewItem == QueueItemTreeItem"
        },
        {
          "command": "cloudmusic.deleteSong",
          "when": "view == queue && viewItem == QueueItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.deleteSong",
          "when": "view == queue && viewItem == QueueItemTreeItem"
        },
        {
          "command": "cloudmusic.playNext",
          "when": "view == queue && viewItem == QueueItemTreeItem",
          "group": "inline"
        },
        {
          "command": "cloudmusic.playNext",
          "when": "view == queue && viewItem == QueueItemTreeItem"
        },
        {
          "command": "cloudmusic.deleteFromPlaylist",
          "when": "view == userPlaylist && viewItem == QueueItemTreeItem || view == favoritePlaylist && viewItem == QueueItemTreeItem"
        },
        {
          "command": "cloudmusic.saveToPlaylist",
          "when": "viewItem == QueueItemTreeItem"
        },
        {
          "command": "cloudmusic.songDetail",
          "when": "viewItem == QueueItemTreeItem"
        },
        {
          "command": "cloudmusic.playlistDetail",
          "when": "viewItem == PlaylistItemTreeItem"
        },
        {
          "command": "cloudmusic.songComment",
          "when": "viewItem == QueueItemTreeItem"
        },
        {
          "command": "cloudmusic.playlistComment",
          "when": "viewItem == PlaylistItemTreeItem"
        }
      ]
    },
    "configuration": [
      {
        "title": "Cloudmusic",
        "properties": {
          "cloudmusic.account.autoCheck": {
            "type": "boolean",
            "default": false,
            "markdownDescription": "%cloudmusic.configuration.cloudmusic.account.autoCheck.markdownDescription%",
            "scope": "window"
          },
          "cloudmusic.cache.size": {
            "type": "number",
            "default": 1024,
            "minimum": 128,
            "maximum": 10240,
            "markdownDescription": "%cloudmusic.configuration.cloudmusic.cache.size.markdownDescription%",
            "scope": "window"
          },
          "cloudmusic.cache.localDirectory": {
            "type": "string",
            "default": "",
            "format": "uri",
            "markdownDescription": "%cloudmusic.configuration.cloudmusic.cache.localDirectory.markdownDescription%",
            "scope": "window"
          },
          "cloudmusic.music.realIP": {
            "type": "string",
            "default": "",
            "format": "ipv4",
            "markdownDescription": "%cloudmusic.configuration.cloudmusic.music.realIP.markdownDescription%",
            "scope": "window"
          },
          "cloudmusic.music.quality": {
            "type": "number",
            "default": 192000,
            "markdownDescription": "%cloudmusic.configuration.cloudmusic.music.quality.markdownDescription%",
            "enum": [
              128000,
              192000,
              320000,
              999000
            ],
            "enumDescriptions": [
              "%cloudmusic.configuration.cloudmusic.music.quality.enumDescriptions.low%",
              "%cloudmusic.configuration.cloudmusic.music.quality.enumDescriptions.medium%",
              "%cloudmusic.configuration.cloudmusic.music.quality.enumDescriptions.high%",
              "%cloudmusic.configuration.cloudmusic.music.quality.enumDescriptions.lossless%"
            ],
            "scope": "window"
          },
          "cloudmusic.player.mediaControl": {
            "type": "boolean",
            "default": true,
            "markdownDescription": "%cloudmusic.configuration.cloudmusic.player.mediaControl.markdownDescription%",
            "scope": "window"
          },
          "cloudmusic.player.defaultLibrary": {
            "type": "string",
            "default": "Rodio",
            "markdownDescription": "%cloudmusic.configuration.cloudmusic.player.defaultLibrary.markdownDescription%",
            "enum": [
              "Rodio",
              "Miniaudio"
            ],
            "scope": "window"
          }
        }
      }
    ]
  },
  "scripts": {
    "build": "gulp build && parcel build webview/index.tsx --no-source-maps && webpack --mode production",
    "build-dev": "gulp build && parcel build webview/index.tsx --no-minify && webpack --mode development",
    "build-native": "electron-build-env neon build --release",
    "build-native-dev": "electron-build-env neon build",
    "lint": "eslint src && eslint webview",
    "test": "tsc -p src",
    "vscode:prepublish": "yarn build"
  },
  "devDependencies": {
    "@types/lodash.throttle": "4.1.6",
    "@types/lru-cache": "5.1.0",
    "@types/node": "12.19.6",
    "@types/react": "16.9.56",
    "@types/react-dom": "17.0.0",
    "@types/vscode": "1.49.0",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "babel-eslint": "10.1.0",
    "electron": "9.3.3",
    "electron-build-env": "0.2.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "gulp": "4.0.2",
    "gulp-filter": "6.0.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "neon-cli": "0.5.2",
    "parcel": "2.0.0-nightly.447",
    "prettier": "2.2.0",
    "sass": "1.29.0",
    "ts-loader": "8.0.11",
    "typescript": "4.1.2",
    "vscode-nls-dev": "3.3.2",
    "webpack": "5.6.0",
    "webpack-cli": "4.2.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "NeteaseCloudMusicApi": "git+https://github.com/YXL76/NeteaseCloudMusicApi.git#self-use",
    "antd": "^4.8.4",
    "array-unsort": "^1.1.6",
    "bluebird": "3.7.2",
    "cacache": "^15.0.5",
    "lru-cache": "^6.0.0",
    "md5-file": "^5.0.0",
    "node-cache": "^5.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "vscode-nls": "^5.0.0"
  }
}
